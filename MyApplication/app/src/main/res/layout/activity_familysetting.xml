<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/FamilyImageView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/familyship" />

    <Button
        android:id="@+id/FamilyImageUpload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="imageUpload(opengallery)"
        />

    <EditText
        android:id="@+id/editTextFamilyname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="가족 이름 정하기"
        android:inputType="textPersonName"
        android:text="Name" />

    <Button
        android:id="@+id/FamilynameDefine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FamilynameDefine" />
</LinearLayout>